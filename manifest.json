{
  "short_name": "Giftlist Extension",
  "name": "Giftlist extension",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "http://*/*", 
    "https://*/*"
  ],
  "browser_action": {
    "default_popup": "/build/index.html",
    "default_icon": {
      "16": "/public/images/logo_16.png",
      "32": "/public/images/logo_32.png",
      "48": "/public/images/logo_48.png",
      "128": "/public/images/logo_128.png" 
    }
  },
  "icons": {
    "16": "/public/images/logo_16.png",
    "32": "/public/images/logo_32.png",
    "48": "/public/images/logo_48.png",
    "128": "/public/images/logo_128.png"
  },
  "content_security_policy": "script-src 'self' 'sha256-fdAfMKZtCTRPKMRznGwonJHC++vCCrQS7XFnVrzSXAE=' 'sha256-YD8w9VLrdvqI5/tFVmpT+1337ly+e/JvPrkRxYZo/w4='; object-src 'self';"
}